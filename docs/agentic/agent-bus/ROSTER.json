{
  "schemaVersion": 2,
  "sessionName": "agentic-cockpit",
  "orchestratorName": "orchestrator",
  "daddyChatName": "daddy",
  "autopilotName": "autopilot",
  "agents": [
    {
      "name": "daddy",
      "role": "daddy-chat",
      "kind": "codex-chat",
      "workdir": "$REPO_ROOT",
      "startCommand": "bash scripts/agentic/codex-chat-supervisor.sh",
      "skills": ["cockpit-daddy-chat-io", "cockpit-agentbus"]
    },
    {
      "name": "orchestrator",
      "role": "orchestrator-worker",
      "kind": "node-worker",
      "workdir": "$REPO_ROOT",
      "startCommand": "node scripts/agent-orchestrator-worker.mjs --agent orchestrator --tmux-notify",
      "skills": ["cockpit-orchestrator", "cockpit-agentbus"]
    },
    {
      "name": "autopilot",
      "role": "autopilot-worker",
      "kind": "codex-worker",
      "branch": "agent/autopilot",
      "workdir": "$AGENTIC_WORKTREES_DIR/autopilot",
      "startCommand": "node scripts/agent-codex-worker.mjs --agent autopilot",
      "skills": [
        "cockpit-autopilot",
        "cockpit-pr-review-closure-gate",
        "cockpit-continuity-ledger",
        "cockpit-skillops",
        "cockpit-agentbus",
        "code-change-verification",
        "cockpit-planning",
        "cockpit-exec-agent"
      ]
    },
    {
      "name": "qa",
      "role": "codex-worker",
      "kind": "codex-worker",
      "branch": "agent/qa",
      "workdir": "$AGENTIC_WORKTREES_DIR/qa",
      "startCommand": "node scripts/agent-codex-worker.mjs --agent qa",
      "skills": ["cockpit-qa", "cockpit-agentbus", "code-change-verification", "cockpit-planning", "cockpit-exec-agent"]
    },
    {
      "name": "frontend",
      "role": "codex-worker",
      "kind": "codex-worker",
      "branch": "agent/frontend",
      "workdir": "$AGENTIC_WORKTREES_DIR/frontend",
      "startCommand": "node scripts/agent-codex-worker.mjs --agent frontend",
      "skills": ["cockpit-agentbus", "code-change-verification", "cockpit-planning", "cockpit-exec-agent"]
    }
  ]
}
